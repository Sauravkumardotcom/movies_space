# Vercel Environment Variables Template
# Copy these to your Vercel project settings and fill in with actual values

# API Configuration
VITE_API_BASE_URL=https://your-vercel-url.vercel.app/api

# Google Drive Integration
VITE_GOOGLE_DRIVE_API_KEY=your_google_drive_api_key_here
VITE_GOOGLE_DRIVE_FOLDER_ID=your_google_drive_folder_id_here

# EmailJS Integration
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id_here
VITE_EMAILJS_TEMPLATE_ID=your_emailjs_template_id_here
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key_here

# Backend Configuration (if needed)
NODE_ENV=production
PORT=5000
